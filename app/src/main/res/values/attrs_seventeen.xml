<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="Seventeen">
        <attr name="widthInch" format="float" />
        <attr name="intensity" format="float" />
        <attr name="string" format="string" />
        <attr name="nStroke" format="integer" />
        <attr name="fontStyle">
            <enum name="NORMAL" value="0" />
            <enum name="BOLD" value="1" />
            <enum name="ITALIC" value="2" />
            <enum name="BOLD_ITALIC" value="3" />
        </attr>
        <attr name="fontFamily" format="string">
            <enum name="DEFAULT" value="0" />
            <enum name="DEFAULT_BOLD" value="1" />
            <enum name="MONOSPACE" value="2" />
            <enum name="SANS_SERIF" value="3" />
            <enum name="SERIF" value="4" />
        </attr>
    </declare-styleable>
</resources>
