<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gmail.takashi316.tminchart.conchart.ConChartFragment"
    android:background="#ffffffff"
    >

    <!-- TODO: Update blank fragment layout -->

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollViewTconChart" >

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/horizontalScrollViewTconChart" >

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left|top"
                android:id="@+id/tableLayoutTconChart"></TableLayout>
        </HorizontalScrollView>
    </ScrollView>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="この計測（tConChart）を終了し\n次の計測（tMinChart）を開始する"
        android:id="@+id/buttonStartTminChartFragment"
        android:layout_gravity="right|bottom"
        android:textSize="30dp" />


</FrameLayout>
